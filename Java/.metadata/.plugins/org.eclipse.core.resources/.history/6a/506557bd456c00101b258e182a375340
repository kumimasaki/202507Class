	public static void main(String[] args) {
		// リストの宣言
		List<String> fruits = new ArrayList<String>();
		fruits.add("apple");
		fruits.add("banana");
		fruits.add("cherry");
		fruits.add("date");
		fruits.add("elderberry");
		fruits.add("fig");
		fruits.add("grape");
		
				List<String> fruits2 = List.of("apple", "banana", "cherry", 
						"date", "elderberry", "fig", "grape");
